    "snyk": "1.1300.2",
    "tailwindcss": "3.4.18",
    "tsx": "^4.7.0",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "schema": "src/lib/prisma/schema.prisma",
    "seed": "tsx scripts/seed.ts"
  },
  "keywords": [
    "clinic",
    "healthcare",
    "medical",
    "appointment",
    "booking",
