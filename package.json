    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "k6": "0.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
